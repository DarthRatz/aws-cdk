{
  "name": "@aws-cdk/asset-manifest-schema",
  "version": "1.22.0",
  "description": "Schema definition for the Asset Manifest",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.assetmanifestschema",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "cdk-asset-manifest-schema"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AssetManifestSchema",
        "packageId": "Amazon.CDK.AssetManifestSchema",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk",
        "iconUrl": "https://raw.githubusercontent.com/aws/aws-cdk/master/logo/default-256-dark.png"
      },
      "python": {
        "distName": "aws-cdk.asset-manifest-schema",
        "module": "aws_cdk.asset_manifest_schema"
      }
    }
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "build+test+package": "npm run build+test && npm run package",
    "build+test": "npm run build && npm test",
    "compat": "cdk-compat"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "dependencies": {
    "semver": "^7.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "statements": 80
      }
    },
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html",
      "text-summary"
    ]
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "cdk-build-tools": "1.22.0",
    "jest": "^24.9.0",
    "pkglint": "1.22.0"
  },
  "repository": {
    "url": "https://github.com/aws/aws-cdk.git",
    "type": "git",
    "directory": "packages/@aws-cdk/asset-manifest-schema"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "homepage": "https://github.com/aws/aws-cdk",
  "bundledDependencies": [
    "semver"
  ],
  "engines": {
    "node": ">= 10.3.0"
  },
  "stability": "stable"
}
